datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Database {
  id        String   @id @default(cuid())
  name      String
  type      String
  host      String
  port      Int
  username  String
  password  String
  database  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
} 